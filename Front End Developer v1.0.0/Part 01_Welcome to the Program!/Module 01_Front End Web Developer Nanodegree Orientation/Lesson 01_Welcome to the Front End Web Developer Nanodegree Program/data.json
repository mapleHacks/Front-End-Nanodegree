          "title": "Access the Career Portal",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "99232e4b-299c-40f5-a985-3162da96288e",
            "completed_at": null,
            "last_viewed_at": null,
